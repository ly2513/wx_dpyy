/**login.wxss**/
.login_button{
	width:10em;
	height:3em;
	margin-bottom:10%;
	font-size:large;
	line-height:3em;
	border: 4rpx #fff solid;
	letter-spacing:3rpx;
	float: left;
}
.text{
	margin-top:6%;
	color:#fff;
}
.explain{
	color:#fff;
	padding-top:10rpx
}
.background {
	width: 100%;
	height: 100%;
	position:fixed; 
	background-size:100% 100%;
	z-index: -1;
}
.logo-box{
	text-align: center;
}
.logo_img{ 
	padding-top: 15%; 
	height:100px;
	margin: 0 auto;
	width: 100%;
}
.agree-box{
	width:100%;
	height:100%;
	display:flex;
	flex-direction: column;
	justify-content:center;
	align-items:center;
	align-content:center
}
.agree-button{
	border-radius: 10rpx;
	margin-top:40%;
	background-color:#FFFFFF;
	color:#1B294C
}

