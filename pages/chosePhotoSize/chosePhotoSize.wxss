/* pages/newPlaceOrder.wxss */
.background {
	width: 100%;
	height: 200rpx;
	position: fixed;
	z-index: -1;
}
.row_photo{
	display: flex;
	width: 90%;
	height: 120rpx;
	justify-content:center;
	align-items: center;
	flex-direction: row;
	margin-bottom: 20rpx;
}
.photo{
	height:120rpx
}
.photo_padding{
	padding-left:20rpx;
}
.column_photo{
	display: flex;
	width: 100%;
	justify-content:center;
	align-items: center;
	flex-direction: column;
}
.content_bg {
	padding-top: 40rpx;
	display: flex;
	flex-direction: column;
	width: 100%;
	height: 100%;
	align-items: center
}
.form {
	margin-top: 10%;
	width: 90%;
	display: flex;
	flex-direction: column;
	border-radius: 25px;
	background-color: white;
}
.first_line{
	display: flex;
	flex-direction: row;
	width: 100%;
	height: 150rpx;
	align-items: center;
}