	font-size:35rpx;
	line-height: 78rpx;
}
.modal_line{
	width:100%;
	height: 1px;
	color: #E6E6E6;
	background-color: #E6E6E6;
}
.store_middle_box{
	width:100%;
	height:7%;
	padding-left:30rpx;
	margin-top:20rpx;
	display:flex; 
	flex-direction: row;
}
.modal_box{
	margin-top:30rpx; 
	width:100%;
	height:45rpx;
	display:flex;
	justify-content:center;
	font-size:35rpx
}
.modal_line{
	height: 1.5rpx;
	color: #E6E6E6;
	background-color: #E6E6E6;
	width:100%;
	margin-top:30rpx;
	display:flex;
	flex-direction: column
}
.modal_textarea{
	height: 40%;
    width: 80%;
    left: 10%;
