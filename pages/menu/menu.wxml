<!-- menu.wxml -->
<view>
  	<image class='background' src="/images/v2.0/background.png" mode="aspectFill"></image>
	<view class="logo-box">
		<image class="logo_img" src="/images/logo.png" mode="aspectFill"></image>
	</view>
	<view class="cent_layout">
		<view class="layout_horizontal">
			<view class="layout_vertical">
				<image class="print_order" src="/images/print_btn.png" bindtap="placeOrder"></image>
				<image class="share_bt" src="/images/share_library_btn.png" bindtap="complaintProposal"></image>
			</view>
			<view class="layout_vertical">
				<image class="other_btn" src="/images/order_search_btn.png" bindtap="myOrder"></image>
        <image class="smart_bt" src="/images/smart_photo.png" bindtap="smart_photo"></image>
			</view>
		</view>
	</view>
	<view class="text">
		<text >达派创投旗下直营品牌</text>
	</view>
</view>